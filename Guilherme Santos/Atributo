package Personagem.Roles;

import Personagem.Personagem;
import Personagem.Roles.Assassino.Assassino;
import Personagem.Roles.Atirador.Atirador;
import Personagem.Roles.Lutador.Lutador;
import Personagem.Roles.Mago.Mago;
import Personagem.Roles.Suporte.Suporte;
import Personagem.Roles.Tank.Tank;

public abstract class Roles extends Personagem {
    private Tank tank;
    private Mago mago;
    private Atirador atirador;
    private Assassino assassino;
    private Suporte suporte;
    private Lutador lutador;

    public Roles(String nome, Tank tank, Mago mago, Atirador atirador, Assassino assassino, Suporte suporte, Lutador lutador) {
        super("");
        this.atirador = atirador;
        this.assassino = assassino;
        this.lutador = lutador;
        this.mago = mago;
        this.suporte = suporte;
        this.tank = tank;
    }


    public Roles(String nome) {
        super("");
    }
}



