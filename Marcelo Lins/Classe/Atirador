package Personagem.Roles.Atirador;

import Personagem.Roles.*;
import Personagem.Roles.Atirador.Subclasse.*;
public class Atirador extends Roles {
    private Gunner gunner;
    private Hunter hunter;
    private DeadEye deadeye;

    public Atirador(Gunner gunner, Hunter hunter, DeadEye deadeye){
        super("");
        this.gunner = gunner;
        this.hunter = hunter;
        this.deadeye = deadeye;
    }

    public void habilidadePassiva() {
        System.out.println(getNome() + "");
    }

    public void atributoPassivo() {
        System.out.println(getNome() + "");
    }
}


