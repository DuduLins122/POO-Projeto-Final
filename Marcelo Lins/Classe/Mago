package Personagem.Roles.Mago;

import Personagem.Roles.Mago.Subclasse.*;
import Personagem.Roles.*;



public class Mago extends Roles {
    private BlackMage blackmage;
    private Summoner summoner;
    private Necromancer necromancer;

    public Mago(BlackMage blackmage, Summoner summoner, Necromancer necromancer) {
        super("");
        this.blackmage = blackmage;
        this.summoner = summoner;
        this.necromancer = necromancer;

    }

    public void habilidadePassiva() {
        System.out.println(getNome() + "");
    }

    public void atributoPassivo() {
        System.out.println(getNome() + "");
    }

}

