
package Personagem.Roles.Assassino;

import Personagem.Roles.Assassino.Subclasse.*;
import Personagem.Roles.*;


public class Assassino extends Roles {
    private Samurai samurai;
    private Reaper reaper;
    private Viper viper;

    public Assassino(Samurai samurai, Reaper reaper, Viper viper){
        super("");
        this.samurai = samurai;
        this.reaper = reaper;
        this.viper = viper;
    }

    public void habilidadePassiva() {
        System.out.println(getNome() + "");
    }

    public void atributoPassivo() {
        System.out.println(getNome() + "");
    }
}

