package Personagem.Racas;

import Personagem.Atributo.Atributo;
import java.io.Serializable;

public abstract class Raca implements Serializable {
    private static final long serialVersionUID = 1L;

    public Raca() {

    }

    public abstract void aplicarBonusAtributos(Atributo atributo);

    public abstract void descricaoPassivaRaca();
}
